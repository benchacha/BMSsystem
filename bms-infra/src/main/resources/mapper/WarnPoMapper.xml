<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bench.bms.infra.repository.mapper.WarnPoMapper">

    <resultMap id="warnResultMap" type="com.bench.bms.infra.repository.entity.WarnPo">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="carId" column="car_idr" jdbcType="BIGINT"/>
        <result property="batteryType" column="battery_type" jdbcType="VARCHAR"/>
        <result property="warnName" column="warn_name" jdbcType="VARCHAR"/>
        <result property="warnLevel" column="warn_level" jdbcType="INTEGER"/>
    </resultMap>

    <insert id="save">
        INSERT INTO Warn (car_id, battery_type, warn_name, warn_level)
        VALUES (#{carId}, #{batteryType}, #{warnName}, #{warnLevel})
    </insert>



</mapper>